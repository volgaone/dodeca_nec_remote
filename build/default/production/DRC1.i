;--------------------------------------------------------------------
;
; Salvataggio e recupero di registri di uso comune quando
; si chiama una funzione o una macro
;
;--------------------------------------------------------------------

save_registers:
    movwf wreg_temp ;
    movf BSR,W ;
    movwf bsr_temp ;
    return ;

restore_registers:
    movf bsr_temp,W ;
    movwf BSR ;
    movf wreg_temp,W ;
    return ;

